{
  "name": "@theanswer/copilot-auto",
  "version": "1.0.0",
  "description": "Make AWS Copilot deployments easier with intelligent configuration management and automated workflows",
  "license": "MIT",
  "type": "module",
  "bin": {
    "copilot-auto": "./dist/esm/bin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/deploy.js",
      "require": "./dist/cjs/deploy.cjs",
      "types": "./dist/esm/deploy.d.ts"
    }
  },
  "files": [
    "dist",
    "templates",
    "scripts",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/bin.ts",
    "start": "node ./dist/esm/bin.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "demo:setup": "node scripts/demo-setup.js",
    "demo:deploy": "node scripts/demo-deploy.js",
    "demo:cleanup": "node scripts/demo-cleanup.js"
  },
  "keywords": [
    "aws",
    "copilot",
    "deployment",
    "devops",
    "docker",
    "ecs",
    "cloudformation",
    "theanswer",
    "aai"
  ],
  "author": {
    "name": "TheAnswer AI",
    "email": "dev@theanswer.ai",
    "url": "https://theanswer.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theanswer/copilot-auto-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/theanswer/copilot-auto-deploy/issues"
  },
  "homepage": "https://github.com/theanswer/copilot-auto-deploy#readme",
  "dependencies": {
    "commander": "^12.1.0",
    "execa": "^9.3.0",
    "prompts": "^2.4.2",
    "js-yaml": "^4.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
